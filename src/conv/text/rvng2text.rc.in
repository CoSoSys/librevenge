	VALUE "ProductName", "librevenge"
	VALUE "ProductVersion", "@RVNG_MAJOR_VERSION@.@RVNG_MINOR_VERSION@.@RVNG_MICRO_VERSION@"
      END
    END
    BLOCK "VarFileInfo"
